{
  "name": "back_end",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch ./src/server.ts",
    "start": "node ./build/server.js",
    "build": "tsup src --out-dir build",
    "test": "vitest",
    "test:unit": "vitest tests/unit",
    "test:e2e": "vitest tests/e2e",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:setup": "npm run db:test:reset && npm run db:test:migrate",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset",
    "db:test:reset": "cross-env DATABASE_URL=$TEST_DATABASE_URL npx prisma migrate reset --force",
    "db:test:migrate": "cross-env DATABASE_URL=$TEST_DATABASE_URL npx prisma migrate deploy",
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^3.2.4",
    "cross-env": "^7.0.3",
    "cuid": "^3.0.0",
    "prisma": "^6.9.0",
    "supertest": "^7.1.1",
    "tsup": "^8.0.2",
    "tsx": "^4.20.3",
    "typescript": "^5.4.3",
    "uuid": "^11.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/multipart": "^9.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.9.0",
    "@types/bcrypt": "^5.0.2",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^20.8.0",
    "@types/nodemailer": "^6.4.17",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "dayjs": "^1.11.13",
    "fastify": "^5.3.3",
    "fastify-type-provider-zod": "^5.0.1",
    "google-auth-library": "^10.1.0",
    "googleapis": "^150.0.1",
    "moment-timezone": "^0.6.0",
    "nodemailer": "^7.0.3",
    "zod": "^3.25.64"
  },
